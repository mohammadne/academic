<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXListView?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<?import javafx.scene.layout.HBox?>
<BorderPane prefHeight="400.0" prefWidth="400.0" stylesheets="../../../../../resources/style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="http_client.ui.main.center_panel.header.HeaderController">
    <center>
        <JFXListView fx:id="listView" layoutX="188.0" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
    </center>
   <bottom>
       <HBox>
           <JFXButton onMouseClicked="#removeHeaderFiled" text="Delete Header" BorderPane.alignment="CENTER">
               <graphic>
                   <FontAwesomeIcon glyphName="TRASH" />
               </graphic>
           </JFXButton>
           <JFXButton onMouseClicked="#onTapAddNewHeader" text="Add A New Header" BorderPane.alignment="CENTER">
               <graphic>
                   <FontAwesomeIcon glyphName="PLUS" />
               </graphic>
           </JFXButton>
       </HBox>

   </bottom>
</BorderPane>
